{
	"name": "vml-starter-angular",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"config": "ts-node src/environments/set-env.ts",
		"start": "node server.js",
		"start:dev": "npm run config && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --configuration=development",
		"heroku-prebuild": "cp .npmrc.example .npmrc",
		"heroku-postbuild": "echo 'Doing nothing'",
		"build": "npm run config && ng build",
		"watch": "ng build --watch --configuration development",
		"test": "npm run config && ng test",
		"lint": "npm run lint:ts && npm run lint:styles",
		"lint:ts": "eslint \"src/**/*.ts\"",
		"lint:styles": "stylelint \"src/**/*.scss\"",
		"lint:fix": "npm run lint:ts -- --fix && npm run lint:styles -- --fix"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^20.2.4",
		"@angular/cdk": "^20.2.9",
		"@angular/common": "^20.2.4",
		"@angular/compiler": "^20.2.4",
		"@angular/core": "^20.2.4",
		"@angular/forms": "^20.2.4",
		"@angular/platform-browser": "^20.2.4",
		"@angular/platform-browser-dynamic": "^20.2.4",
		"@angular/router": "^20.2.4",
		"@datorama/akita": "^7.1.1",
		"@datorama/akita-ng-router-store": "^7.0.0",
		"@okta/okta-auth-js": "^6.8.1",
		"@okta/okta-signin-widget": "^6.7.0",
		"@primeng/themes": "^20.2.0",
		"@tailwindcss/postcss": "^4.1.14",
		"@wppopen/components-library": "^2.15.0",
		"@wppopen/core": "9.0.0",
		"class-validator": "^0.13.2",
		"colors": "^1.4.0",
		"compression": "^1.7.4",
		"dotenv": "^16.0.2",
		"express": "^4.18.1",
		"express-sslify": "^1.2.0",
		"helmet": "^6.0.0",
		"penpal": "^6.2.2",
		"primeicons": "^7.0.0",
		"primeng": "^20.2.0",
		"rxjs": "~6.6.0",
		"tailwindcss-primeui": "^0.6.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.3.0",
		"typescript": "~5.8.3",
		"zone.js": "~0.15.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^20.2.2",
		"@angular/cli": "^20.2.2",
		"@angular/compiler-cli": "^20.2.4",
		"@eslint/js": "^9.0.0",
		"@types/express": "^5.0.6",
		"@types/jasmine": "^6.0.0",
		"@types/lodash": "^4.17.23",
		"@types/multer": "^1.4.12",
		"@types/uuid": "^10.0.0",
		"angular-eslint": "^19.0.0",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.0.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-security": "^3.0.1",
		"eslint-plugin-sonarjs": "^3.0.6",
		"jasmine-core": "~3.8.0",
		"karma": "~6.3.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.0.3",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "~1.7.0",
		"postcss": "^8.5.6",
		"stylelint": "^16.0.0",
		"stylelint-config-standard-scss": "^14.0.0",
		"typescript-eslint": "^8.0.0"
	},
	"browser": {
		"typeorm": "node_modules/typeorm/typeorm-model-shim.js",
		"@nestjs/swagger": "./shim/api-property.shim.js",
		"class-transformer": "./shim/class-transformer.shim.js",
		"crypto": false,
		"fs": false,
		"os": false,
		"url": false,
		"child_process": false,
		"assert": false,
		"stream": false,
		"http": false,
		"https": false,
		"dns": false,
		"tls": false,
		"net": false,
		"zlib": false,
		"mock-aws-s3": false,
		"nock": false,
		"constants": false,
		"@mapbox/node-pre-gyp": false,
		"node-gyp": false,
		"bcrypt": false,
		"process": "./shim/process.shim.js"
	}
}
